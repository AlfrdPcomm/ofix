    "slider-layout#poweredby"
  ]
  
  },

  "flex-layout.col#socialmobile":{
    "children":[
      "rich-text#social",
      "flex-layout.row#smmob"
    ],"props":{
      "widt":"100%",
      "blockClass":"socialmob",
      "horizontalAlign":"center"
    }
  },
  "flex-layout.col#metpamobile":{
    "children":[
      "slider-layout#pmethod"
    ],"props":{
      "blockClass":"methpaymob"
    }
  },
  
  
  
  
  
  
  "flex-layout.col#social":{
    "children":[
      "rich-text#social"
      
    ],"props":{
      "blockClass":"tituloSiguenos",
      "horizontalAlign":"center"
    }
  },
"rich-text#social":{
